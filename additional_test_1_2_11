#include <iostream>
#include <cmath>
#include <iomanip>
using namespace std;

int main ()
{
int x1,x2,x3,y1,y2,y3;
double p;
cout << "Koordinati pervoi tochki:";
cin >> x1 >> y1;
cout << "Koordinati vtoroi tochki:";
cin >> x2 >> y2;
cout << "Koordinati tretei tochki:";
cin >> x3 >> y3;
p=(sqrt((pow(x2-x1,2))+(pow(y2-y1,2))))+(sqrt((pow(x3-x2,2))+(pow(y3-y2,2))))+(sqrt((pow(x1-x3,2))+(pow(y1-y3,2))));
cout << "Perimetr:"<<p;
return 0;
}
